      function adjustAge(
        todayYear,
        todayMonth,
        todayDay,
        birthYear,
        birthMonth,
        birthDay
      ) {
        let adjustedYears = todayYear - birthYear - 1;
        let adjustedMonths = 12 - birthMonth + todayMonth;
        let adjustedDays = 0;

        if (todayDay >= birthDay) {
          adjustedDays = todayDay - birthDay;
        } else {
          adjustedMonths--;
